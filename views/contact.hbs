<div class="container" style="margin-top: 50px;">
    <h1>Following people want to contact</h1>
    <br>
    {{#each contact}}
    <div class="row">
        <div class="col-md-9">
            <div class="card p-4 m-2 alert-danger" style="width: 25rem;">

                <div class="card-body">
                    <h4 class="card-title">{{name}}</h4>
                    <h6 class="card-title">{{email}}</h6>
                    <h6 class="card-title">{{mobile}}</h6>
                   

                </div>



            </div>
        </div>

    </div>

    <br><br>
    {{/each}}

</div>